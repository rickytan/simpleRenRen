{
  "name": "simple RenRen",
  "description": "人人网页面越来越复杂，在浮澡的时代，您应当拥有更清爽的信息",
  "version": "0.5.0",
  "manifest_version": 2,
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "http://www.renren.com/"
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["http://www.renren.com/*"],
      "css": ["simple.css"]
    }
  ]
}